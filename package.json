{
  "name": "question-it",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "npm run prod",
    "start": "npm run prod",
    "start-server": "node index",
    "dev": "run-s dev-prepare dev-start",
    "dev-start": "run-p dev-client dev-server dev-run",
    "dev-prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.js prepare",
    "dev-client": "webpack-dev-server --port 8080 --host 0.0.0.0 --hot --inline --progress --color --config ./webpack/webpack.config.client.js --display-error-details -d",
    "dev-server": "webpack --watch --config ./webpack/webpack.config.server.js --colors --display-error-details -d",
    "dev-run": "nodemon index --watch ./public/build/server",
    "prod": "run-s prod-client prod-server prod-run",
    "prod-run": "node index",
    "prod-client": "webpack --progress --colors -p --config ./webpack/webpack.config.client.js",
    "prod-server": "webpack --progress --colors --config ./webpack/webpack.config.server.js",
    "debug": "run-s debug-client debug-server prod-run",
    "debug-client": "webpack --progress --colors -d --config ./webpack/webpack.config.client.js",
    "debug-server": "webpack --progress --colors -d --config ./webpack/webpack.config.server.js",
    "prebuild": "npm run clean",
    "build": "webpack --progress --color",
    "clean": "del /Q /S .\\public\\build\\*",
    "devrun": "start node index & npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/papigers/question-it.git"
  },
  "author": "Gershon Papi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/papigers/question-it/issues"
  },
  "homepage": "https://github.com/papigers/question-it#readme",
  "dependencies": {
    "bluebird": "^3.4.0",
    "express": "^4.13.4",
    "history": "^2.1.1",
    "material-ui": "^0.15.0",
    "npm-run-all": "^2.1.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "scriptjs": "^2.5.8",
    "source-map-support": "^0.4.0",
    "universal-webpack": "^0.1.11"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.5.2",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-decorators": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^3.7.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
